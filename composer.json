{
    "name": "netgen/layouts-core",
    "description": "Netgen Layouts is an easy and flexible way of managing all page layouts on your website. This is the core of Netgen Layouts, its heart and soul.",
    "license": "MIT",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "netgen/layouts-ui": "~1.0.0",
        "netgen/content-browser": "~1.0.0",
        "symfony/asset": "^3.4.24 || ^4.2.5",
        "symfony/config": "^3.4.24 || ^4.2.5",
        "symfony/console": "^3.4.24 || ^4.2.5",
        "symfony/debug": "^3.4.24 || ^4.2.5",
        "symfony/dependency-injection": "^3.4.24 || ^4.2.5",
        "symfony/doctrine-bridge": "^3.4.24 || ^4.2.5",
        "symfony/event-dispatcher": "^3.4.24 || ^4.2.5",
        "symfony/expression-language": "^3.4.24 || ^4.2.5",
        "symfony/form": "^3.4.24 || ^4.2.5",
        "symfony/framework-bundle": "^3.4.24 || ^4.2.5",
        "symfony/http-foundation": "^3.4.24 || ^4.2.5",
        "symfony/http-kernel": "^3.4.24 || ^4.2.5",
        "symfony/intl": "^3.4.24 || ^4.2.5",
        "symfony/monolog-bundle": "^3.3",
        "symfony/options-resolver": "^3.4.24 || ^4.2.5",
        "symfony/polyfill-php73": "^1.10",
        "symfony/proxy-manager-bridge": "^3.4.24 || ^4.2.5",
        "symfony/routing": "^3.4.24 || ^4.2.5",
        "symfony/security-core": "^3.4.24 || ^4.2.5",
        "symfony/security-csrf": "^3.4.24 || ^4.2.5",
        "symfony/security-bundle": "^3.4.24 || ^4.2.5",
        "symfony/serializer": "^3.4.24 || ^4.2.5",
        "symfony/translation": "^3.4.24 || ^4.2.5",
        "symfony/twig-bridge": "^3.4.24 || ^4.2.5",
        "symfony/twig-bundle": "^3.4.24 || ^4.2.5",
        "symfony/validator": "^3.4.24 || ^4.2.5",
        "symfony/yaml": "^3.4.24 || ^4.2.5",
        "twig/twig": "^2.9",
        "twig/extensions": "^1.5.4",
        "sensio/framework-extra-bundle": "^5.3",
        "doctrine/dbal": "^2.9.2",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/collections": "^1.6",
        "ramsey/uuid": "^3.8",
        "egulias/email-validator": "^2.1",
        "ezyang/htmlpurifier": "^4.10",
        "xemlock/htmlpurifier-html5": ">=0.1.10, <1.0",
        "knplabs/knp-menu-bundle": "^2.2",
        "swaggest/json-schema": "^0.12",
        "friendsofsymfony/http-cache-bundle": "^1.3.16 || ^2.6.1",
        "php-http/socket-client": "^1.4",
        "white-october/pagerfanta-bundle": "^1.2.4",
        "jean85/pretty-package-versions": "^1.2",
        "emodric/twig-package-versions": "^1.0",
        "nikolaposa/version": "^3.1"
    },
    "require-dev": {
        "netgen/layouts-standard": "~1.0.0",
        "phpunit/phpunit": "^8.1",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/var-dumper": "^3.4.24 || ^4.2.5",
        "symfony/web-server-bundle": "^3.4.24 || ^4.2.5",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "coduo/php-matcher": "^3.1",
        "phpspec/php-diff": "^1.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "matthiasnoback/symfony-config-test": "^4.0.1",
        "lchrusciel/api-test-case": "^4.1",
        "webmozart/assert": "^1.4",
        "behat/behat": "^3.5",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.3",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.2",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/page-object-extension": "^0.3",
        "franzl/studio": "*"
    },
    "conflict": {
        "ocramius/proxy-manager": "<2.1"
    },
    "config": {
        "process-timeout": 3600
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "test-api": "@php vendor/bin/phpunit -c phpunit-api.xml --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always",
        "coverage-api": "@php -dzend_extension=xdebug.so vendor/bin/phpunit -c phpunit-api.xml --coverage-html=coverage_api --colors=always",
        "coverage-clover": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-clover=coverage.xml --colors=always",
        "coverage-clover-api": "@php -dzend_extension=xdebug.so vendor/bin/phpunit -c phpunit-api.xml --coverage-clover=coverage_api.xml --colors=always",
        "behat": "@php vendor/bin/behat --config tests/behat/behat.yml --colors --strict --no-interaction -vvv -f progress",
        "behat-headless": "@php vendor/bin/behat --config tests/behat/behat.yml --colors --strict --no-interaction -vvv -f progress -p headless",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundles migrations",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=7 --ansi tests"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Layouts\\": "lib/",

            "Netgen\\Layouts\\Migrations\\": "migrations/",

            "Netgen\\Bundle\\LayoutsBundle\\": "bundles/LayoutsBundle/",
            "Netgen\\Bundle\\LayoutsAdminBundle\\": "bundles/LayoutsAdminBundle/",
            "Netgen\\Bundle\\LayoutsDebugBundle\\": "bundles/LayoutsDebugBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\Layouts\\Tests\\": "tests/lib/",
            "Netgen\\Layouts\\Behat\\": "tests/behat/",
            "Netgen\\Layouts\\Tests\\App\\": "tests/application/src/",
            "Netgen\\Bundle\\LayoutsBundle\\Tests\\": "tests/bundles/LayoutsBundle/",
            "Netgen\\Bundle\\LayoutsAdminBundle\\Tests\\": "tests/bundles/LayoutsAdminBundle/",
            "Netgen\\ContentBrowser\\Tests\\": "vendor/netgen/content-browser/tests/lib/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
